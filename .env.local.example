# API Configuration
# Set to 'true' to use mock API (no backend required)
# Set to 'false' to use real backend API
NEXT_PUBLIC_USE_MOCK_API=true

# Real API Base URL (used when NEXT_PUBLIC_USE_MOCK_API=false)
NEXT_PUBLIC_API_BASE_URL=http://localhost:8080

# EDC Management API endpoint
NEXT_PUBLIC_EDC_MANAGEMENT_URL=http://localhost:8080/management/v3
